{
  "extends": ["react-app", "plugin:prettier/recommended"],
  "plugins": ["react-hooks"],
  "rules": {
    "import/no-webpack-loader-syntax": 0,
    "import/first": 0,
    "react-hooks/rules-of-hooks": 2,
    "react-hooks/exhaustive-deps": 0,
    "prettier/prettier": 1,
    "array-callback-return": 0
  }
}
