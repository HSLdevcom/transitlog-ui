  "scripts": {
    "docker:build": "docker build -t hsldevcom/transitlog-ui .",
    "docker:run": "docker run -d -p 3000:3000 hsldevcom/transitlog-ui",
    "start": "node scripts/start.js",
    "start:production": "NODE_ENV=production node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "test:ci": "CI=true node scripts/test.js",
    "test:staged": "CI=true node scripts/test.js --bail --findRelatedTests",
    "codestyle": "prettier \"src/**/*.{css,js}\" --write",
    "lint": "eslint \"src/**/*.js\" --fix",
    "production": "forever start -c \"yarn run serve -s -l 3000\" build && forever logs -f 0",
    "prepare": "patch-package",
    "codegen": "gql-gen --config codegen.yml",
    "cypress": "cypress open"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@graphql-codegen/cli": "^1.2.1",
    "@graphql-codegen/fragment-matcher": "^1.7.0",
    "@graphql-codegen/introspection": "^1.2.1",
    "@graphql-codegen/typescript": "^1.2.1",
    "babel-plugin-styled-components": "^1.10.1",
    "cypress": "^3.5.0",
    "cypress-commands": "^0.3.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-cypress": "^2.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react-hooks": "^2.0.1",
    "husky": "^3.0.5",
    "jest-dom": "^4.0.0",
    "jest-environment-jsdom-thirteen": "^1.0.1",
    "jest-styled-components": "^6.3.3",
    "lint-staged": "^9.3.0",
    "mobx-react-devtools": "^6.0.3",
    "patch-package": "^6.2.0",
    "postinstall-prepare": "^1.0.1",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.8.6"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.css": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "automock": false,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom",
      "<rootDir>/src/setupTests"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(spec|test).{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-thirteen",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
